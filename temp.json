{
  "schema": 2,
  "global": {
    "data": { "count_multiplier": 1, "dest": "ubuntu182" },
    "transform-pre": {
      "script": [
        "  .reference.before = \"This was inserted first.\"",
        "| .reference.sponsor = $global.sponsor",
        "| .reference.iteration = $iteration"
      ]
    },
    "transform-post": {
      "script": [
        "  .reference.after = \"This was inserted last.\"",
        "| if (.test.spec | has(\"dest\"))",
        "    then .test.spec.dest = $global.dest",
        "    else . end"
      ]
    }
  },
  "jobs": [
    {
      "label": "label",
      "parallel": true,
      "task": [
        {
          "type": "throughput",
          "spec": { "dest": "perfsonar-bin-seb.umnet.umich.edu" }
        }
      ]
    },
    {
      "label": "label",
      "parallel": true,
      "task": [
        {
          "type": "http",
          "spec": { "url": "http://portal-mguest-lb.dsc.umich.edu" }
        }
      ]
    }
  ]
}
